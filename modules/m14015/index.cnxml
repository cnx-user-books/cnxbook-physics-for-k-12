<m:mn> 5 </m:mn>
<m:msqrt>
<m:mn> 2 </m:mn>
</m:msqrt>

</m:mtd>
</m:mtr>

<m:mtr>
<m:mtd>
<m:msub>
<m:mi> v </m:mi>
<m:mi> y </m:mi>
</m:msub>
