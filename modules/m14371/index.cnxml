<m:msub>
<m:mi mathvariant="bold"> v </m:mi>
<m:mi> R </m:mi>
</m:msub>
</m:mtd>
</m:mtr>
</m:mtable>
</m:math>
</para>
<para id="element-60">
<m:math display="block">
<m:mtable columnalign="left">
<m:mtr>
<m:mtd>
<m:msub>
<m:mi mathvariant="bold"> v </m:mi>
<m:mi> P </m:mi>
</m:msub>
<m:mo> = </m:mo>
<m:mo> ( </m:mo>
<m:mi> v </m:mi>
<m:mo> + </m:mo>
<m:mi> ω </m:mi>
<m:mi> r </m:mi>
<m:mi> cos </m:mi>
<m:mi> θ </m:mi>
<m:mo> ) </m:mo>
<m:mi mathvariant="bold"> i </m:mi>
<m:mo> - </m:mo>
<m:mi> ω </m:mi>
<m:mi> r </m:mi>
<m:mi> sin </m:mi>
<m:mi> θ </m:mi>
<m:mo> ) </m:mo>
