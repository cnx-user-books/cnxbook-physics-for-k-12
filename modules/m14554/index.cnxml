</m:msup>
<m:mo> - </m:mo>
<m:mn> 8 </m:mn>
<m:mi> t </m:mi>
<m:mo> - </m:mo>
<m:mn> 12 </m:mn>
<m:mo> = </m:mo> 
<m:mn> 0 </m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mi> t </m:mi>
<m:mo> = </m:mo>
<m:mfrac>
<m:mrow>
<m:mo> - </m:mo>
<m:mo> ( </m:mo>
