<m:mtr>
<m:mtd>
<m:msub>
<m:mi> x </m:mi>
<m:mi> n </m:mi>
</m:msub>
<m:mo> = </m:mo> 
<m:mi> u </m:mi>
<m:mo> + </m:mo>
<m:mfrac>
<m:mi> a </m:mi>
<m:mn> 2 </m:mn>
</m:mfrac>
<m:mo> ( </m:mo>
<m:mn> 2 </m:mn>
<m:mi> n </m:mi>
<m:mo> - </m:mo>
<m:mn> 1 </m:mn>
<m:mo> ) </m:mo>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:msub>
<m:mi> x </m:mi>
<m:mi> n </m:mi>
</m:msub>
<m:mo> = </m:mo> 
<m:mn> 9 </m:mn>
<m:mo> + </m:mo>
<m:mfrac>
<m:mrow>
<m:mo> - </m:mo>
<m:mn> 2 </m:mn>
</m:mrow>
<m:mn> 2 </m:mn>
</m:mfrac>
<m:mo> ( </m:mo>
<m:mn> 2 </m:mn>
<m:mspace width="2pt"/>
<m:mo> x </m:mo> 
