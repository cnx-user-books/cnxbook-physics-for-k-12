        </m:msub>
      </m:mrow>
    </m:mfenced>
    <m:mo>+</m:mo>
    <m:msub>
      <m:mi>v</m:mi>
      <m:mrow>
        <m:msub>
          <m:mi>i</m:mi>
          <m:mn>1</m:mn>
        </m:msub>
      </m:mrow>
    </m:msub>
    <m:mfenced>
      <m:mrow>
        <m:msub>
          <m:mi>m</m:mi>
          <m:mn>1</m:mn>
        </m:msub>
        <m:mo>+</m:mo>
        <m:mi>e</m:mi>
        <m:msub>
          <m:mi>m</m:mi>
          <m:mn>1</m:mn>
        </m:msub>
      </m:mrow>
    </m:mfenced>
  </m:mrow>
</m:math>
</para>
<para id="element-42">
<m:math display="block">
  <m:mrow>
    <m:mo>⇒</m:mo>
    <m:msub>
      <m:mi>v</m:mi>
      <m:mrow>
        <m:mn>2</m:mn>
        <m:mi>f</m:mi>
      </m:mrow>
    </m:msub>
    <m:mo>=</m:mo>
    <m:mrow>
      <m:mfrac>
        <m:mrow>
          <m:mfenced>
            <m:mrow>
              <m:msub>
                <m:mi>m</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
              <m:mo>−</m:mo>
              <m:mi>e</m:mi>
              <m:msub>
                <m:mi>m</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
            </m:mrow>
          </m:mfenced>
        </m:mrow>
        <m:mrow>
          <m:mfenced>
            <m:mrow>
              <m:msub>
                <m:mi>m</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
              <m:mo>+</m:mo>
              <m:msub>
                <m:mi>m</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
            </m:mrow>
          </m:mfenced>
        </m:mrow>
      </m:mfrac>
    </m:mrow>
    <m:msub>
      <m:mi>v</m:mi>
      <m:mrow>
        <m:mn>2</m:mn>
        <m:mi>i</m:mi>
      </m:mrow>
    </m:msub>
    <m:mo>+</m:mo>
    <m:mfrac>
      <m:mrow>
        <m:mfenced>
          <m:mrow>
            <m:msub>
              <m:mi>m</m:mi>
              <m:mn>1</m:mn>
            </m:msub>
            <m:mo>+</m:mo>
            <m:mi>e</m:mi>
